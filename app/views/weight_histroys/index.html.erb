<p id="notice"><%= notice %></p>

<h1>Weight Histroys</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Weight</th>
      <th>Memo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weight_histroys.each do |weight_histroy| %>
      <tr>
        <td><%= weight_histroy.user_id %></td>
        <td><%= weight_histroy.weight %></td>
        <td><%= weight_histroy.memo %></td>
        <td><%= link_to 'Show', weight_histroy %></td>
        <td><%= link_to 'Edit', edit_weight_histroy_path(weight_histroy) %></td>
        <td><%= link_to 'Destroy', weight_histroy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weight Histroy', new_weight_histroy_path %>
